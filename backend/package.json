{
  "name": "ai-assign-eval-backend",
  "version": "0.1.0",
  "description": "Backend API for AI Assignment Evaluation System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently -n api,web -c green,cyan \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:frontend": "npm --prefix ../frontend run dev -- --host --port 3000 --open",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
